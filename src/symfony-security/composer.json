{
    "name": "web-auth/webauthn-symfony-security-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "FIDO2/Webauthn Security Bundle For Symfony",
    "keywords": ["FIDO", "FIDO2", "U2F", "webauthn", "Symfony", "Security"],
    "homepage": "https://github.com/web-auth",
    "authors": [
        {
            "name": "Florent Morselli",
            "homepage": "https://github.com/Spomky"
        },
        {
            "name": "All contributors",
            "homepage": "https://github.com/web-auth/webauthn-symfony-security-bundle/contributors"
        }
    ],
    "require": {
        "symfony/security-bundle": "^4.1",
        "web-auth/webauthn-symfony-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webauthn\\Security\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webauthn\\Security\\Bundle\\Tests\\Functional\\": "tests/functional/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
